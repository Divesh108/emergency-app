
{
  "expo": {
    "name": "BlackOps Emergency",
    "slug": "blackops-emergency",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blackops.emergency",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app requires location access to send your location during emergencies",
        "NSLocationAlwaysUsageDescription": "This app requires location access to send your location during emergencies",
        "NSLocationWhenInUseUsageDescription": "This app requires location access to send your location during emergencies",
        "NSMicrophoneUsageDescription": "This app requires microphone access to record audio during emergencies",
        "NSCameraUsageDescription": "This app requires camera access to record video during emergencies"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.blackops.emergency",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_SMS",
        "android.permission.SEND_SMS",
        "android.permission.RECEIVE_SMS",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}